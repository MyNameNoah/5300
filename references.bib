	title = {Electronic Structure Calculations for Solids and Molecules: Theory and Computational Methods},
	url = {http://www.amazon.ca/exec/obidos/redirect?tag=citeulike09-20\&amp;path=ASIN/0521815916},
	year = {2006}
}
  
